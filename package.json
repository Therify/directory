{
    "name": "app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "postinstall": "yarn prisma:generate",
        "dev:doppler": "doppler run --project=directory --config=dev --token=$DEV_DIRECTORY_DOPPLER_TOKEN -- yarn dev",
        "dev": "next dev",
        "dev:all": "doppler run -p=directory -c=dev -- run-p dev storybook test:watch prisma:studio stripe-cli:listen",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:e2e": "jest --config ./e2e/jest.config.ts",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "prisma:init": "prisma init",
        "prisma:generate": "prisma generate",
        "prisma:dbpush": "doppler run --project=directory --config=dev --token=$DEV_DIRECTORY_DOPPLER_TOKEN -- yarn prisma db push",
        "prisma:studio": "prisma studio",
        "prisma:migrate": "prisma migrate deploy",
        "vercel-build": "prisma generate && prisma migrate deploy && next build",
        "stripe-cli:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
        "localtunnel": "lt --port 3000"
    },
    "dependencies": {
        "@auth0/nextjs-auth0": "^2.1.0",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@fontsource/roboto": "^4.5.8",
        "@hookform/resolvers": "^3.0.0",
        "@mui/icons-material": "^5.11.0",
        "@mui/material": "^5.11.3",
        "@mui/x-date-pickers": "^5.0.13",
        "@next/font": "13.1.1",
        "@prisma/client": "^4.9.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@trpc/client": "^9.25.3",
        "@trpc/next": "^9.27.0",
        "@trpc/react": "^9.27.0",
        "@trpc/server": "^9.27.0",
        "@types/node": "18.11.18",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "axios": "^1.3.2",
        "cloudinary": "^1.33.0",
        "date-fns": "^2.29.3",
        "dotenv": "^16.0.3",
        "env-var": "^7.3.0",
        "eslint": "8.31.0",
        "eslint-config-next": "^13.1.1",
        "firebase": "^9.16.0",
        "firebase-admin": "^11.5.0",
        "framer-motion": "^8.4.3",
        "inngest": "^0.9.3",
        "jest": "^29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "jest-mock-extended": "2.0.4",
        "mailing-core": "^0.9.10",
        "micro": "^10.0.1",
        "neverthrow": "^6.0.0",
        "next": "^13.1.1",
        "nookies": "^2.5.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.41.5",
        "react-lottie": "^1.2.3",
        "react-query": "3",
        "stream-chat": "^8.4.1",
        "stream-chat-react": "^10.6.0",
        "stripe": "^11.6.0",
        "typescript": "4.9.4",
        "usehooks-ts": "^2.9.1",
        "uuid": "^9.0.0",
        "zod": "^3.20.2"
    },
    "devDependencies": {
        "@anatine/zod-mock": "^3.8.3",
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@faker-js/faker": "^7.6.0",
        "@storybook/addon-essentials": "^7.0.0-beta.19",
        "@storybook/addon-interactions": "^7.0.0-beta.19",
        "@storybook/addon-links": "^7.0.0-beta.19",
        "@storybook/blocks": "^7.0.0-beta.19",
        "@storybook/nextjs": "^7.0.0-beta.19",
        "@storybook/react": "^7.0.0-beta.19",
        "@storybook/testing-library": "^0.0.13",
        "@testing-library/user-event": "^14.4.3",
        "@types/react-lottie": "^1.2.6",
        "@types/uuid": "^9.0.0",
        "autoprefixer": "^10.4.13",
        "eslint-plugin-storybook": "^0.6.8",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.0",
        "localtunnel": "^2.0.2",
        "mailing": "^0.9.10",
        "prisma": "^4.9.0",
        "storybook": "^7.0.0-beta.19",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "zod-prisma": "^0.5.4"
    },
    "lint-staged": {
        "**/*.{ts,tsx}": [
            "prettier src/ --write",
            "eslint src/ --fix",
            "jest --bail --findRelatedTests --passWithNoTests"
        ]
    }
}
